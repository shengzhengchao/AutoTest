      },
      "headers": { "Content-Type": "text/plain; charset=GBK" }
    }

  },
  {
    "description": "这是一个带cookies信息的get请求",
    "request": {
      "uri": "/get/with/cookies",
      "method": "get",
      "cookies": {
        "login": "true"
      }
    },
    "response": {
      "headers": { "Content-Type": "text/plain; charset=GBK" },
