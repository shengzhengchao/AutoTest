[
  {
    "description":"这是一个带headers的post请求",
    "request":{
      "uri":"/post/with/headers",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "json": {
        "name": "Jack",
        "sex": "woman"
      }
    },
    "response":{
      "headers": { "Content-Type": "text/plain; charset=GBK" },
      "json": {
        "status": "success",
        "code": "1",
        "name": "Jack"
      }
    }
  }


]